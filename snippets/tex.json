{

  "Skeleton": {
    "prefix": "skeleton",
    "body": "\\documentclass{${1:type}}\n\n\\begin{document}\n\n$0\n\n\\end{document}",
    "description": "Create a document skeleton"
  },

  "Environment": {
    "prefix": "environment",
    "body": "\\begin{${1:env}}\n\t$0\n\\end{${1}}",
    "description": "Create the start and end of an environment"
  },

  "Fraction": {
    "prefix": "fraction",
    "body": "\\frac{\n\t${1:numerator}\n}{\n\t${2:denominator}\n}",
    "description": "Create a fraction in math mode"
  },

  "Derivative": {
    "prefix": "derivative",
    "body": "\\frac{\n\td ${1:dependent}\n}{\n\td ${2:variable}\n}",
    "description": "Create a derivative in math mode"
  },

  "Partial Derivative": {
    "prefix": "partial",
    "body": "\\frac{\n\t\\partial ${1:dependent}\n}{\n\t\\partial ${2:variable}\n}",
    "description": "Create a partial derivative in math mode"
  }

}
